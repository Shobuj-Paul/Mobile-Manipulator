  <selfCollide>true</selfCollide>
</gazebo>

<gazebo reference="wrist_1">
  <mu1>${joint_friction}</mu1>
  <mu2>${joint_friction}</mu2>
  <selfCollide>true</selfCollide>
</gazebo>

<gazebo reference="palm_1">
  <mu1>${joint_friction}</mu1>
